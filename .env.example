# ========================================
# Kiwoom API Configuration - Paper Trading
# ========================================

# === 인증 정보 ===
KIWOOM_APP_KEY=your_kiwoom_app_key_here
KIWOOM_APP_SECRET=your_kiwoom_app_secret_here
KIWOOM_ACCOUNT_NO=your_kiwoom_account_no_here

# === 키움 Paper Trading 설정 ===
# 키움증권 공식 Paper Trading URL (모의투자)
KIWOOM_API_DOMAIN=mockapi.kiwoom.com
KIWOOM_API_URL=https://mockapi.kiwoom.com
KIWOOM_REST_API_URL=https://mockapi.kiwoom.com
KIWOOM_WEBSOCKET_URL=wss://mockapi.kiwoom.com:10000

# === 모드 설정 (개발용) ===
# Paper Trading 모드 (모의투자) - 실제 키움 API 사용시 활성화
KIWOOM_PAPER_MODE=true
# Production 모드 (실거래 - 주의!)
KIWOOM_PRODUCTION_MODE=false

# === 거래 관련 설정 ===
KIWOOM_ENABLE_TRADING=true

# API KEYs
TAVILY_API_KEY=your_tavily_api_key_here
OPENAI_API_KEY=your_openai_api_key_here
NAVER_CLIENT_ID=your_naver_client_id_here
NAVER_CLIENT_SECRET=your_naver_client_secret_here

# ECOS(한국은행 경제 통계시스템) API KEY
ECOS_API_KEY=your_ecos_api_key_here

# DART(금융감독원 전자공시시스템) API KEY
DART_API_KEY=your_dart_api_key_here

# FRED(연방준비제도) API KEY
FRED_API_KEY=your_fred_api_key_here

# === 거래 제한 및 보안 설정 ===
KIWOOM_MAX_ORDER_AMOUNT=1000000

# === 디버깅 및 로깅 설정 ===
KIWOOM_DEBUG_MODE=false
KIWOOM_LOG_LEVEL=INFO

LANGSMITH_TRACING=true
LANGSMITH_API_KEY=your_langsmith_api_key_here
LANGSMITH_PROJECT=default